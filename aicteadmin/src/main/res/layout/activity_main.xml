<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/mydraw"
    tools:context=".MainActivity"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/framelayout"
            />


        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/nav_bottomview"
            app:menu="@menu/adminbottomnav_menu"
            android:background="#191970"
            app:itemIconTint="@color/yellow"
            app:itemActiveIndicatorStyle="@color/a"
            app:itemTextAppearanceActive="@color/a"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/nav_bottomview1"
            app:menu="@menu/adminbottomnav_menu"
            android:background="#191970"
            app:itemIconTint="@color/yellow"
            app:itemTextAppearanceActive="@color/a"
            app:itemActiveIndicatorStyle="@color/a"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#a4dded"
        app:headerLayout="@layout/nav_header"
        app:menu="@menu/adminnav_menu"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"

        >

    </com.google.android.material.navigation.NavigationView>







</androidx.drawerlayout.widget.DrawerLayout>
